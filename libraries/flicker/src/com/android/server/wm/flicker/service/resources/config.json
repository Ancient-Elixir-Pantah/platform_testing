{
  "assertors": [
    {
      "name": "com.android.server.wm.flicker.service.assertors.RotationAssertions",
      "transition": "ROTATION",
      "assertions": {
        "presubmit": {
          "wmTrace": [
            "navBarWindowIsVisible",
            "visibleWindowsShownMoreThanOneConsecutiveEntry",
            "statusBarWindowIsAlwaysInvisible"
          ],
          "layersTrace": [
            "navBarLayerIsVisible",
            "entireScreenCovered",
            "statusBarLayerIsAlwaysInvisible",
            "visibleLayersShownMoreThanOneConsecutiveEntry"
          ]
        },
        "postsubmit": {
          "wmTrace": [],
          "layersTrace": []
        },
        "flaky": {
          "wmTrace": [],
          "layersTrace": [
            "navBarLayerRotatesAndScales"
          ]
        }
      }
    }
  ]
}